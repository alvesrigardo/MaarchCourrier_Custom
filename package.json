{
  "name": "maarchcourrier",
  "version": "21.03.1",
  "description": "MaarchCourrier",
  "scripts": {
    "ng": "ng",
    "ng-update": "ng update --allow-dirty --force",
    "build-prod": "ng build --project \"maarchCourrier\" --configuration production",
    "build-watch": "ng build --project \"maarchCourrier\" --watch",
    "build-prod-addin-outlook": "ng build --project \"addin-outlook\" --configuration production",
    "build-addin-outlook": "ng build --project \"addin-outlook\"",
    "build": "ng build --project \"maarchCourrier\"",
    "build-release": "npm run build-prod && npm run build-prod-addin-outlook && npm run reload-packages-prod && npm run reload-packages-prod-addin-outlook",
    "check-update": "npm outdated",
    "dep-list": "npm list --depth=0",
    "upgrade": "npm upgrade --save",
    "lint": "eslint -c .eslintrc.js --ext .ts src/frontend/",
    "lint-fix": "eslint -c .eslintrc.js --ext .ts src/frontend/ --fix",
    "reload-packages": "rm -Rf node_modules && rm -f package-lock.json && npm cache clean --force && npm config set legacy-peer-deps true && npm i",
    "reload-packages-addin-outlook": "cd plugins/addin-outlook && rm -Rf node_modules && rm -f package-lock.json && npm cache clean --force && && npm config set legacy-peer-deps true && npm i",
    "reload-packages-prod": "rm -Rf node_modules && rm package-lock.json && npm cache clean --force && npm config set legacy-peer-deps && npm i --production",
    "reload-packages-prod-addin-outlook": "cd plugins/addin-outlook && rm -Rf node_modules && rm package-lock.json && npm cache clean --force && npm config set legacy-peer-deps && npm i --production",
    "create-component": "echo Component name ? && read varname && ng g c $varname --module app --style=scss",
    "create-component-admin": "echo Component name ? && read varname && ng g c administration/$varname --module administration --style=scss",
    "cypress:open": "cypress open --config-file test/e2e/cypress-config.json",
    "cypress:run": "cypress run --browser firefox --headless --config-file test/e2e/cypress-config.json"
  },
  "keywords": [],
  "author": "Maarch",
  "license": "GPL-3.0",
  "dependencies": {
    "@types/node": "^12.12.6",
    "angular2-draggable": "^2.3.2",
    "core-js": "^3.15.1",
    "tinymce": "^5.8.2",
    "tinymce-i18n": "^20.12.25",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.2",
    "@angular-eslint/eslint-plugin": "^14.0.2",
    "@angular/animations": "^14.1.2",
    "@angular/cdk": "^14.1.2",
    "@angular/cli": "^14.1.2",
    "@angular/common": "^14.1.2",
    "@angular/compiler": "^14.1.2",
    "@angular/compiler-cli": "^14.1.2",
    "@angular/core": "^14.1.2",
    "@angular/forms": "^14.1.2",
    "@angular/material": "^14.1.2",
    "@angular/platform-browser": "^14.1.2",
    "@angular/platform-browser-dynamic": "^14.1.2",
    "@angular/platform-server": "^14.1.2",
    "@angular/router": "^14.1.2",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@swimlane/ngx-charts": "^20.1.0",
    "@types/datatables.net": "^1.10.19",
    "@types/jquery": "^3.5.5",
    "@types/jquery.nicescroll": "^3.7.1",
    "@types/jstree": "^3.3.40",
    "@types/tooltipster": "0.0.30",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "ajv": "^7.2.4",
    "bootstrap": "^3.4.1",
    "chosen-js": "^1.8.7",
    "codelyzer": "^6.0.2",
    "cypress": "^6.9.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "flat-to-nested": "^1.1.1",
    "hammerjs": "^2.0.8",
    "html-minifier": "^4.0.0",
    "jquery": "^3.6.0",
    "jquery-typeahead": "^2.11.1",
    "jquery.nicescroll": "^3.7.6",
    "jstree-bootstrap-theme": "^1.0.1",
    "ng2-dnd": "^5.0.2",
    "ng2-pdf-viewer": "^6.4.1",
    "ngx-color": "^6.2.0",
